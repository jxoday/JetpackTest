<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinxin.jetpacktest">

    <application
        android:name=".lifecycle.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name=".lifecycle.service.MyService" />

        <activity android:name="com.jinxin.jetpacktest.room.RoomTestActivity" />
        <activity android:name=".livedata.LiveDataFragmentActivity" />
        <activity android:name=".livedata.LiveDataTestActivity" />
        <activity android:name=".viewmodel.ViewModelTestActivity" />
        <activity android:name=".navigation.NavigationActivity">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>

            <nav-graph android:value="@navigation/nav_graph" />
        </activity>
        <activity android:name=".lifecycle.LifecycleMainActivity" />
        <activity android:name=".lifecycle.service.LifecycleServiceActivity" />
        <activity android:name=".lifecycle.activity.LifeCycleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>